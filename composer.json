{
  "name": "cocolis/woocommerce",
  "description": "Cocolis Woocommerce Plugin to add Cocolis.fr as a delivery method",
  "type": "wordpress-plugin",
  "require": {
    "cocolis/php": "dev-develop"
  },
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Cocolis",
      "email": "contact@cocolis.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\Cocolis\\": "src/"
    }
  },
  "scripts": {
    "wp-package": "zip -r cocolis.zip cocolis"
  },
  "config": {
    "vendor-dir": "cocolis/vendor"
  },
  "minimum-stability": "dev"
}